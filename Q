#include <stdio.h>

int main()
{
	int n;
	scanf("%d",&n);

	for(int i =0; i<=n-1; ++i) {
		for(int j=0; j<=n-1; ++j) {
		
		if((i==j&& i>=n/2)||(j==0 && i!=0 && i<=n/2 ) ||( j==(n/2)+1 && i!=0 && i<=n/2 ) ||(i==0 && j!=0 && j<=n/2 ) || (i==(n/2)+1 && j!=0 && j<=n/2 ) )
			{
				printf("*");
			}
			else {
				printf(" ");
			}
		}
		printf("\n");
	}

	return 0;
}




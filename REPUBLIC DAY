#include <stdio.h>

int main()
{
    int n;
    scanf("%d",&n);
    
    
    
//happy
    for(int i =0; i<=n-1; ++i) {
        
        //h
        for(int j=0; j<=n-1; ++j) {
            if(j==0|| j==n-1 ||(i==n/2 ))
            {
                printf("H");
            }
            else {
                printf(" ");
            }
        }
        printf("    ");
        //a
        
        for(int j=0; j<=n-1; ++j) {
            if((i==0&&j!=0&&j!=n-1) || j==0&&i!=0||  j==n-1&&i!=0|| i==n/2)
            {
                printf("A");
            }
            else {
                printf(" ");
            }
        }
        printf("    ");
        
        //p
        
        for(int j=0; j<=n-1; ++j) {
            if(j==0 || (i==0 && j!=n-1) || (i==n/2 && j!=n-1) ||( j==n-1 && i>0 && i<n/2))
            {
                printf("P");
            }
            else {
                printf(" ");
            }
        }
        printf("    ");
        
        //p
        
        for(int j=0; j<=n-1; ++j) {
            if(j==0 || (i==0 && j!=n-1) || (i==n/2 && j!=n-1) ||( j==n-1 && i>0 && i<n/2))
            {
                printf("P");
            }
            else {
                printf(" ");
            }
        }
        printf("    ");
        
        
        //y
        for(int j=0; j<=n-1; ++j) {
            if((i==j && i<n/2) ||i+j == n-1)
            {
                printf("Y");
            }
            else {
                printf(" ");
            }
        }
        printf("\n");
    }
    
    
    printf("\n\n\n");  // row maine space dene le liye 
    
    
  //REPUBLIC
        for(int i=0; i<=n-1; ++i) {
    //R
            
            
        for(int j=0; j<=n-1; ++j) {

        if(j==0|| (i==0&& j<n/2) || (i==n/2&& j<n/2) || i-j==n/2 || (j==n/2 && i!=0 & i!=n/2 && i<=n/2 )){
                printf("R");
            }
            else {
                printf(" ");
            }
        }
        // printf("    ");  
        
    
            //E
            
            for(int j=0; j<=n-1; ++j) {
            if(i==0|| j==0 ||i==n-1 ||(i==n/2 &&j<=n/2))
            {
                printf("E");
            }
            else {
                printf(" ");
            }
        }
        printf("    ");
            
      //P
            
            
                for(int j=0; j<=n-1; ++j) {
            if(j==0 || (i==0 && j!=n-1) || (i==n/2 && j!=n-1) ||( j==n-1 && i>0 && i<n/2))
            {
                printf("P");
            }
            else {
                printf(" ");
            }
        }
        printf("    ");
            
            //U
            
                for(int j=0; j<=n-1; ++j) {

        if((j==0 && i!=n-1) ||(j==n-1 && i!=n-1) ||( i==n-1 && j!=0 && j!=n-1 ))
            {
                printf("U");
            }
            else {
                printf(" ");
            }
        }
        printf("    ");
            
            
            //B
            
            
            
            for(int j=0; j<=n-1; ++j) {
            
        
            if(j==0 || (j==n-1 && i!=0 &&i!=n/2&&i!=n-1)|| ((i==n/2|| i==0 ||i==n-1)&&j!=n-1)){
                printf("B");
            }
            else {
                printf(" ");
            }
        }
        printf("    ");
            
            //L
            
            for(int j=0; j<=n-1; ++j) {
            if(j==0|| i==n-1)
            {
                printf("L");
            }
            else {
                printf(" ");
            }
        }
        printf("    ");
            
            
            //I
            
            for(int j=0; j<=n-1; ++j) {
            if(i==0|| i==n-1 ||j==n/2)
            {
                printf("I");
            }
            else {
                printf(" ");
            }
        }
        printf("    ");    
            
            
            //C
            for(int j=0; j<=n-1; ++j) {
            if((i==0 && j!=0)||(j==0&&i!=0&&i!=n-1&&j!=6)||(i==n-1&&j!=0))
            {
                printf("C");
            }
            else {
                printf(" ");
            }
        }
        printf("\n");
} 
    
    
      printf("\n\n\n");  // row maine space dene le liye 
    
    
    //DAY
for(int i =0; i<=n-1; ++i) {
    
   
    //D
    for(int j=0; j<=n-1; ++j) {
            if((i== 0&& j!=n-1)||(i==n-1 && j!=n-1)||(j==n-1 && i!=0 && i!=n-1)|| j==1)
            {
                printf("D");
            }
            else {
                printf(" ");
            }
        }
        printf("    ");    
    
    //a
        
        for(int j=0; j<=n-1; ++j) {
            if((i==0&&j!=0&&j!=n-1) || j==0&&i!=0||  j==n-1&&i!=0|| i==n/2)
            {
                printf("A");
            }
            else {
                printf(" ");
            }
        }
        printf("    ");
    
    
     //y
        for(int j=0; j<=n-1; ++j) {
            if((i==j && i<n/2) ||i+j == n-1)
            {
                printf("Y");
            }
            else {
                printf(" ");
            }
        }
        printf("\n");    
    }
    
   
        printf("\n\n\n");  // row maine space dene le liye 

 
    return 0;
}





